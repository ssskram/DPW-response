@model Contact
<tr>
    <td>
    <input asp-for="Called" type="checkbox" class="form-check-input big-checkbox" id="called" onchange="CheckField()"/>
    </td>
    <td>
    <select asp-for="Accepted" class="form-control" id="accepted" onchange="CheckField()">
        <option value=""></option>
        <option value="Yes">Yes</option>
        <option value="No">No</option>
    </select>
    </td>
    <td>
    <input asp-for="FullName" class="no-border" readonly="true"/>
    </td>
    <td>
    <input asp-for="Department" class="no-border" readonly="true"/>
    </td>
    <td>
    <input asp-for="HomePhone" class="no-border" readonly="true"/>
    </td>
    <td>
    <input asp-for="CellPhone" class="no-border" readonly="true"/>
    </td>
</tr>
   
   

   

   

   

    