@model Contact
<tr>
    <td>
    <input asp-for="FullName" class="no-border" readonly="true" style="width: 155px;"/>
    <input asp-for="OID" hidden="true" id="oid"/>
    </td>
    <td>
    <input asp-for="Department" class="no-border" readonly="true"/>
    </td>
    <td>
    <input asp-for="HomePhone" class="no-border" readonly="true"/>
    </td>
    <td>
    <input asp-for="CellPhone" class="no-border" readonly="true"/>
    <input id="trigger" hidden="true" class="trigger">
    </td>
    <td style="background-color: rgba(136, 130, 149, 0.2);" class="called">
    <select asp-for="Called" data-style="btn-info" class="form-control" id="called" title="select">
        <option></option>
        <option value="Yes">Yes</option>
    </select>
    </td>
    <td style="background-color: rgba(136, 130, 149, 0.2);" class="accepted">
    <select asp-for="Accepted" data-style="btn-info" class="form-control" id="accepted" title="select" disabled="true">
        <option></option>
        <option value="true">Yes</option>
        <option value="false">No</option>
    </select>
    </td>
</tr>
   
   

   

   

   

    